.image-cropper {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  background-color: #000;
}
.image-cropper .img-canvas {
  position: absolute;
  transform: translateX(-100%);
}
.image-cropper .pic-preview {
  width: 100%;
  flex: 1;
  position: relative;
}
.image-cropper .pic-preview .crop-mask-block {
  background-color: rgba(51, 51, 51, 0.8);
  z-index: 2;
  position: fixed;
  box-sizing: border-box;
  pointer-events: none;
}
.image-cropper .pic-preview .crop-circle-box {
  position: fixed;
  box-sizing: border-box;
  z-index: 2;
  pointer-events: none;
  overflow: hidden;
}
.image-cropper .pic-preview .crop-circle-box .crop-circle {
  width: 100%;
  height: 100%;
}
.image-cropper .pic-preview .crop-border {
  position: fixed;
  border: 1px solid #fff;
  box-sizing: border-box;
  z-index: 3;
  pointer-events: none;
}
.image-cropper .pic-preview .crop-grid {
  position: fixed;
  z-index: 3;
  border-style: dashed;
  border-color: #fff;
  pointer-events: none;
  opacity: 0.5;
}
.image-cropper .pic-preview .crop-angle {
  position: fixed;
  z-index: 3;
  border-style: solid;
  border-color: #fff;
  pointer-events: none;
}
.image-cropper .fixed-bottom {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 99;
  display: flex;
  flex-direction: row;
  background-color: #f8f8f8;
}
.image-cropper .fixed-bottom .rotate-icon {
  background-image: url("data:image/png;base64,");
  background-size: 60% 60%;
  background-repeat: no-repeat;
  background-position: center;
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  top: -90rpx;
  left: 10rpx;
  transform: rotateY(180deg);
}
.image-cropper .fixed-bottom .rechoose {
  color: #007aff;
  padding: 0 15px;
  line-height: 100rpx;
}
.image-cropper .fixed-bottom .choose-btn {
  color: #007aff;
  text-align: center;
  line-height: 100rpx;
  flex: 1;
}
.image-cropper .fixed-bottom .button {
  margin: auto 15px auto auto;
  background-color: #007aff;
  color: #fff;
}
.image-cropper .safe-area-inset-bottom {
  padding-bottom: 0;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}